{
  "model": "haiku",
  "hooks": {
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "claude-notify.sh 処理が完了しました"
          }
        ]
      }
    ],
    "Notification": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "jq -r '.message' | xargs -r -I {} claude-notify.sh \"{}\""
          }
        ]
      }
    ]
  },
  "statusLine": {
    "type": "command",
    "command": "~/.claude/statusline.js"
  },
  "enabledPlugins": {
    "forcia-claude-plugin@forcia-claude-marketplace": false,
    "example-skills@anthropic-agent-skills": true
  },
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754309101330
  }
}
